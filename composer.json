{
    "name": "akoepcke/laravel-spawn",
    "description": "Laravel package to generate CRUD and view files",
    "license": "MIT",
    "authors": [
        {
            "name": "A. Koepcke",
            "email": "akoepcke@mail.de"
        }
    ],
    "require": {
        "php": "~7.0",
        "illuminate/support": "^6.14",
        "symfony/process": "^4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "AKoepcke\\LaravelSpawn\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AKoepcke\\LaravelSpawn\\ServiceProvider"
            ]
        }
    }
}
